  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.3",
    "google-maps-react": "^2.0.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-bootstrap": "^1.0.0-beta.2",
    "react-stripe-elements": "^2.0.1",
    "validator": "^10.9.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.6"
  }
